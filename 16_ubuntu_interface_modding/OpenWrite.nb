Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "runTempFile", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"FilePattern", "\[Rule]", "\"\<_\>\""}], ",", 
     RowBox[{"FileExtension", "\[Rule]", "Automatic"}]}], "}"}]}], 
  "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"runTempFile", "[", 
    RowBox[{"command_", ",", "text_", ",", 
     RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"stream", ",", "file", ",", "result"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"stream", "=", 
       RowBox[{"OpenWrite", "[", "]"}]}], ";", 
      RowBox[{"Export", "[", 
       RowBox[{"stream", ",", "text", ",", "\"\<Text\>\""}], "]"}], ";", 
      RowBox[{"file", "=", 
       RowBox[{"Close", "[", "stream", "]"}]}], ";", 
      RowBox[{
       RowBox[{"OptionValue", "[", "FileExtension", "]"}], "/.", 
       RowBox[{
        RowBox[{"ext", ":", 
         RowBox[{"Except", "[", "Automatic", "]"}]}], "\[RuleDelayed]", 
        RowBox[{"(", 
         RowBox[{"file", "=", 
          RowBox[{"RenameFile", "[", 
           RowBox[{"file", ",", 
            RowBox[{"file", "~~", "\"\<.\>\"", "~~", "ext"}]}], "]"}]}], 
         ")"}]}]}], ";", 
      RowBox[{"result", "=", 
       RowBox[{"Import", "[", 
        RowBox[{
         RowBox[{"StringReplace", "[", 
          RowBox[{"command", ",", 
           RowBox[{
            RowBox[{"OptionValue", "[", "FilePattern", "]"}], "\[Rule]", 
            "file"}]}], "]"}], ",", "\"\<Text\>\""}], "]"}]}], ";", 
      RowBox[{"DeleteFile", "[", "file", "]"}], ";", "result"}]}], "]"}]}], 
  ";"}]}], "Input",
